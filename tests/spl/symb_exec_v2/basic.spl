struct DNode {
  var data: Int;
}

procedure loop1(x: DNode)
  requires acc(x) &*& x.data == 0
  ensures acc(x) &*& x.data == 10
{
  var i := 1;
  while (i < 11)
    invariant acc(x) &*& x.data == i - 1 &*& i <= 11
  {
    x.data := i;
    i := i + 1;
  }
}
