options "-symbexec-v2"

struct Node {
   var  data: Int;
   var  next: Node;
}

function f(x: Node, y: Node) returns (z: Int)
  requires acc(x.data) &*& acc(y.data)
  ensures x.data + y.data == z
{
  x.data + y.data
}

