include "../include/node_array.spl";

struct Node {
  var data: Int;
}

procedure destroy(a: Array<Node>, implicit ghost N: Set<Node>)
  requires array_with_content(a, N) &*& acc(N)
  ensures emp
{
  var i := 0;
  while (i < a.length)
    invariant 0 <= i && i <= a.length
    invariant
      exists N2: Set<Node> ::
       (arrayseg_with_content(a, i, a.length, N2) -** array(a)) &*& acc(N2)
  {
    free(a[i]);
    i := i + 1;
  }
  free(a);
}
